<script>
    function handleFileUpload(files) {
        const uploadedAvatar = document.getElementById('uploadedAvatar');
        const file = files[0];
        
        // Check if the file is a JPG or PNG image
        if (file && (file.type === 'image/jpeg' || file.type === 'image/png')) {
            const reader = new FileReader();
            reader.onload = function(e) {
                uploadedAvatar.src = e.target.result;
            };
            reader.readAsDataURL(file);
            
            // Remove any existing error message
            const errorElement = document.getElementById('fileTypeError');
            if (errorElement) {
                errorElement.remove();
            }
            
            // Show the image
            uploadedAvatar.style.display = 'block';
        } else {
            // Create error message element
            const errorElement = document.createElement('div');
            errorElement.className = 'alert alert-danger mt-3 text-start'; // Adjusted alignment
            errorElement.id = 'fileTypeError';
            errorElement.textContent = 'Please upload a JPG or PNG image.';
            
            // Set the width of the error message container to match the text width
            errorElement.style.width = 'fit-content';
            
            // Hide the image
            uploadedAvatar.style.display = 'none';
            
            // Append error message after the image container
            uploadedAvatar.parentNode.parentNode.appendChild(errorElement);
            
            // Reset input file element if needed
            document.getElementById('upload').value = '';
        }
    }
  </script>
  <script>
    // Récupérer la date actuelle
    var currentDate = new Date();
    
    // Formater la date pour qu'elle soit compatible avec le champ de date
    var formattedDate = currentDate.toISOString().split('T')[0];
    
    // Définir la date formatée comme valeur par défaut du champ de date
    document.getElementById('date').value = formattedDate;
  </script>
  


